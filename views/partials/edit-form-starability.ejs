<fieldset class="starability-checkmark">
    <legend>Rating:</legend>

    <button type="button" class="clear-rating">Clear Rating</button>
    
    <input type="radio" id="no-rate-toggle-edit" class="input-no-rate" name="review[rating]" value="0" checked="" aria-label="No rating.">

    <input type="radio" id="toggle-edit-rate1" name="review[rating]" value="1">
    <label for="toggle-edit-rate1" title="Terrible">1 star</label>

    <input type="radio" id="toggle-edit-rate2" name="review[rating]" value="2">
    <label for="toggle-edit-rate2" title="Not good">2 stars</label>

    <input type="radio" id="toggle-edit-rate3" name="review[rating]" value="3">
    <label for="toggle-edit-rate3" title="Average">3 stars</label>

    <input type="radio" id="toggle-edit-rate4" name="review[rating]" value="4">
    <label for="toggle-edit-rate4" title="Very good">4 stars</label>

    <input type="radio" id="toggle-edit-rate5" name="review[rating]" value="5">
    <label for="toggle-edit-rate5" title="Amazing">5 stars</label>

    <span class="starability-focus-ring"></span>
</fieldset>